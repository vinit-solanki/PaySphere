{
    "builds": [
      {
        "src": "frontend/build/**",
        "use": "@vercel/static"
      },
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/index.html"
      }
    ]
  }